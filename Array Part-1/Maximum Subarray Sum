#include <bits/stdc++.h> 
long long maxSubarraySum(int arr[], int n)
{
    long long c_sum=0;
    long long maxsum= LONG_MIN;
   
    for(int i=0; i<n; i++)
    {
        c_sum= c_sum+ arr[i];

        if(c_sum> maxsum)
        {  
             maxsum= c_sum;
        }

        if(c_sum<0 )
        {
            c_sum=0;
        }
        
    }
    return maxsum;
}
